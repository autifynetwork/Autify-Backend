generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Brand {
  id        String   @id @default(cuid())
  email     String   @unique
  name      String?
  whitelist Boolean  @default(false)
  vendors   Brand[]  @relation("VendorBrand")
  vendorOf  Brand[]  @relation("VendorBrand")
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Category {
  id           String   @id @default(cuid())
  categoryName String
  image        String
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
