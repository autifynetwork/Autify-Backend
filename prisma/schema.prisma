generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Brand {
  id        Int     @id @default(autoincrement())
  email     String  @unique
  name      String
  whitelist Boolean @default(false)
}

model Vendor {
  email     String  @unique
  name      String
  whitelist Boolean @default(false)
}
